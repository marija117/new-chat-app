<h1>
  <%= @room.name %>
</h1>

<div class="row">
  <div class="col-12 col-md-3">
    <%= render partial: 'rooms' %>
  </div>

  <div class="col" data-channel-subscribe='chat' data-room-id="<%= @room.id %>">
    <app :room_id="<%= @room.id %>" :current_user="<%= current_user.id %>"></app>
  </div>
</div>